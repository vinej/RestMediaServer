<?xml version = "1.0" encoding = "utf-8" ?>

<hibernate-mapping xmlns = "urn:nhibernate-mapping-2.2"
   assembly = "SqlDAL" namespace = "SqlDAL.Domain">

  <class name = "Member">
    <id name="Id">
      <generator class="guid" />
    </id>
    <property name = "Email"/>
    <property name = "Alias"/>
    <property name = "IsActive"/>
    <property name = "Dob"/>
  </class>

</hibernate-mapping>
